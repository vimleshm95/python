n = int(input("enter the number:: "))
n1= 0
n2= 1
count = 0
if n <= 0:
   print("enter a + integer")
elif n == 1:
   print("Fibonacci series",n,":",n1)
else:
   print("Fibonacci series:")
   while count < n:
       print(n1)
       nth = n1 + n2
       n1 = n2
       n2 = nth
       count += 1
print("program finished")
